type Chart {
    data: [ChartData]
    layout: ChartLayout
}

type ChartData {
    x: [Float]
    y: [Float]
    type: String
    name: String
    mode: String

}

type ChartLayout {
    chartTitle: String
    xAxisLabel: String
    yAxisLabel: String
}

type Query {
    scatterPlot(plateId: ID, xFeatureId: ID, yFeatureId: ID, groupBy: String): Chart
}
